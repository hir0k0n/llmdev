   "source": [
    "price = int(input(\"税抜価格を入力して下さい：\"))\n",
    "\n",
    "tax_price = round(price*1.1)\n",
    "total_price = tax_price\n",
    "\n",
    "if tax_price >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料は350円です\")\n",
    "    total_price += 350\n",
    "\n",
    "print(f\"送料込税込価格は{total_price}円です\")\n"
   ]
  }
